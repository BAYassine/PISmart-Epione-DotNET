@{
    ViewBag.Title = "treatments";
    Layout = "~/Views/Shared/Doctor/_Layout.cshtml";
}




<div class="main-content">

    <section>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <h4 class="title">Basic example</h4>
                    <p>test</p>





                    <div class="body table-responsive">
                        <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                            <thead>
                                <tr>
                                    <th>id</th>
                                    <th>desc</th>
                                    


                                </tr>
                            </thead>

                            <tbody>

                                @foreach (var p in @ViewBag.result)
                                {
                                    <tr>
                                        <th scope="row">@p.id</th>
                                        <td>@p.description</td>
                                        




                                    </tr>

                                }



                            </tbody>

                        </table>


                    </div>
                </div>
            </div>
        </div>
    </section>
</div>








